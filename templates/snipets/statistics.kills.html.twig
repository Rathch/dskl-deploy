<table class="table table-striped text-center caption-top table-hover table-sm generalStatistic">
    <caption><h2 class="text-center">Kills seit 2080</h2></caption>
    <tr>
        <th>#</th>
        <th>Team</th>
        <th>Kills</th>
    </tr>
    {% for TopTenKill in TopTenKills %}
        <tr>
            <td>{{ loop.index }}</td>
            <td>{{ TopTenKill.team.name }}</td>
            <td>{{ TopTenKill.kills  }}</td>
        </tr>
    {% endfor %}
</table>
